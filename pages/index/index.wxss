page{
  width: 100%;
  height: 100%;
}
.container{
  width: 100%;
  height: 100%;
  background-color: #f5f7f9;
}
.section{
  height:108rpx;
  width: 100%;
  background: #ffffff;
  position: fixed;
  top: 0;
  left: 0;
}
.nav {
  height:108rpx;
  width: 550rpx;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 108rpx;
  background: #ffffff;
  font-size: 16px;
  white-space: nowrap;
  position: fixed;
  top: 0;
  left: 100rpx;
  right: 100rpx;
  z-index: 99;
}
.nav-all{
  height:30rpx;
  width: 30rpx;
  position: absolute;
  z-index: 999;
  right: 0rpx;
  top: 0;
  background-color: #fff;
  transform: scale(.18);
  margin-right: 80rpx;
}
.nav-me{
  height:30rpx;
  width: 30rpx;
  left: 0;
  margin-top: 15rpx;
  background-color: #fff;
  position: absolute;
  transform: scale(.15);
  z-index: 999;
}
.nav-item {
  margin-left: 20rpx;
  margin-right: 20rpx;
  display: inline-block;
  text-align: center;
}
.nav-item.active{
  color: #fc975c;
}
.tab-box{
  background: greenyellow;
  padding-top: 80rpx;
  height: 100%;
  box-sizing: border-box;
}
.tab-content{
  overflow-y: scroll;
}